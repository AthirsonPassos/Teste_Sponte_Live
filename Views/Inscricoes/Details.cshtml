                            @Html.DisplayFor(model => model.status_Pag)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Inscrito.nm_Inscrito)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Inscrito.nm_Inscrito)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Live.nm_Live)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Live.nm_Live)
                        </dd>

                    </dl>
                </div>
                <p>
                    @Html.ActionLink("Edita", "Edit", null, new { id = Model.Id_Inscricao }, new { @class = "btn btn-send fa fa-pencil" }) |
                    @Html.ActionLink("Retornar a lista anterior", "Index", null, new { @class = "btn btn-primary" })
                </p>
            </div>
        </div>
    </div>
