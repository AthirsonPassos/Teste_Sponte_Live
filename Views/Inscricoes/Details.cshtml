@model Teste_Sponte_Live.Inscricao

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="wrapper">
    <div class="row">
        <div class="col-md-12">
            <div class="content-panel" style="padding: 20px">
                <h2>Detalhes</h2>

                <div>
                    <h4>Inscricao</h4>
                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.vl_Inscricao)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.vl_Inscricao)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.dt_Vencimento)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.dt_Vencimento)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.status_Pag)
