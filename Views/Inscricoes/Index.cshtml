@model IEnumerable<Teste_Sponte_Live.Inscricao>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="wrapper">
    <div class="row">
        <div class="col-md-12">
            <div class="content-panel" style="padding: 20px">

                <p>
                    @Html.ActionLink("Novo Cadastro", "Create")
                </p>
                <table class="table">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.vl_Inscricao)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.dt_Vencimento)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.status_Pag)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Inscrito.nm_Inscrito)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Live.nm_Live)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.vl_Inscricao)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.dt_Vencimento)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.status_Pag)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Inscrito.nm_Inscrito)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Live.nm_Live)
                            </td>
                            <td>
                                @Html.ActionLink("Editar", "Edit", null, new { id = item.Id_Inscricao }, new { @class = "btn btn-primary fa fa-pencil" }) |
                                @Html.ActionLink("Detalhes", "Details", null, new { id = item.Id_Inscricao }, new { @class = "btn btn-success fa fa-check" }) |
                                @Html.ActionLink("Excluir", "Delete", null, new { id = item.Id_Inscricao }, new { @class = "btn btn-danger fa fa-trash-o" })
                            </td>
                        </tr>
                    }

                </table>
            </div>
        </div>
    </div>
</section>

